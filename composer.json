{
    "name": "ponticlaro/bebop-cms",
    "description": "Bebop library to easily use WordPress CMS capabilities",
    "keywords": ["wordpress", "cms"],
    "type": "library",
    "license": [
        "MIT",
        "GPL-3.0+"
    ],
    "authors": [{
        "name": "Ponticlaro",
        "email": "hello@ponticlaro.com",
        "homepage": "http://www.ponticlaro.com",
        "role": "Developer"
    }],
    "require": {
        "ponticlaro/bebop-common": "^1.1.4",
        "ponticlaro/bebop-ui": "^1",
        "ponticlaro/bebop-scripts-loader": "^1.1"
    },
    "require-dev": {
        "codeception/codeception": "^2",
        "codeception/aspect-mock": "^2",
        "mockery/mockery": "^0",
        "php-mock/php-mock-mockery": "^1",
        "codeclimate/php-test-reporter": "^0",
        "apigen/apigen": "^4"
    },
    "autoload": {
        "psr-4": {
            "Ponticlaro\\Bebop\\Cms\\": "src/"
        }
    },
    "scripts": {
        "generate-docs": "apigen generate --config ./.apigen.yaml"
    },
    "support": {
        "email": "hello@ponticlaro.com",
        "issues": "https://github.com/ponticlaro/bebop-cms/issues",
        "forum": "https://gitter.im/ponticlaro/bebop"
    }
}